{
  "name": "campzama-control",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/app.js",
  "scripts": {
    "start:server": "node dist/server/app.js",
    "dev": "$(npm bin)/run-p dev:*",
    "dev:client": "$(npm bin)/webpack-dev-server --inline --hot --config src/client/webpack.config.js",
    "dev:server": "$(npm bin)/webpack --config src/server/webpack.config.js --watch",
    "build": "$(npm bin)/run-s build:*",
    "build:client": "npm run clean:client && $(npm bin)/webpack --mode=production --config src/client/webpack.config.js",
    "build:server": "npm run clean:server && $(npm bin)/webpack --mode=production --config src/server/webpack.config.js",
    "clean:client": "$(npm bin)/rimraf dist/client/",
    "clean:server": "$(npm bin)/rimraf dist/server/",
    "cpres": "$(npm bin)/cpx ./src/server/payloads/*.json dist/server/payloads/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "log4js": "^2.5.3",
    "passport": "^0.4.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "vue-class-component": "^6.2.0"
  },
  "devDependencies": {
    "@types/helmet": "0.0.37",
    "@types/passport": "^0.4.5",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "app-root-path": "^2.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "nodemon-webpack-plugin": "^4.0.3",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.7.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.1",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.4",
    "vue-property-decorator": "^6.0.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
